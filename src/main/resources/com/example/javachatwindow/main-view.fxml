<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javachatwindow.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <DialogPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="480.0">
       <header>
         <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="320.0">
            <children>
               <TextFlow prefHeight="200.0" prefWidth="200.0" />
            </children></AnchorPane>
       </header>
       <content>
         <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="320.0" />
       </content>
       <expandableContent>
         <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="200.0" prefWidth="320.0" />
       </expandableContent>
   </DialogPane>

    <Label fx:id="welcomeText" />
    <Button contentDisplay="RIGHT" onAction="#onHelloButtonClick" prefHeight="33.0" prefWidth="102.0" text="Отправить" textAlignment="CENTER" textOverrun="WORD_ELLIPSIS" />
</VBox>
